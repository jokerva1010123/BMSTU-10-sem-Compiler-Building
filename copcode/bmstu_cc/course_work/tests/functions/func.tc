int printf(const char *format,...);

void array_print(int array[], int n)
{
    printf("array=[");
    for(int i=0;i<n;i++){
        printf(" %d ", array[i]);
    }
    printf("] and n=%d (number of elements)\n",n);
}

int main()
{
    int array[7] = {3,4,5,1,0,8,9};
    array_print(array, 7);
    return 0;
}